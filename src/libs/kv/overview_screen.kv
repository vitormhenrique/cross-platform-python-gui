#:kivy 2.0
#: import gch kivy.utils.get_color_from_hex


<RallyOverviewScreen>

    canvas:
        Color:
            rgba: gch("33333d")
        Rectangle:
            pos: self.pos
            size: self.size

    MDBoxLayout:
        adaptive_height: True
        orientation: "vertical"
        spacing: dp(80)
        padding: dp(30)
        pos_hint: {"center_x": .5}

        MDLabel:
            id: md_label_title
            text:'this is a test'


        MDTextButton:
            text: 'Test button'
            on_release: root.test()
            pos_hint: {"center_x": .5}

        MDBoxLayout:
            adaptive_height: True
            orientation: "vertical"
            spacing: dp(20)



                
